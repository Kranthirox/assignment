<!DOCTYPE html>
<html lang="en">
<%- include("header") %> 
<body>

     
        <div class="navigation">
            <%- include("nav") %> 
        </div>
        <div class="col">
            <% data.forEach((Post)=>{ %> 
                <div class="card "  style="width: 18rem;">
                    
                    <div class="card-body">
                      <h5 class="card-title"><%= Post.title %></h5>
                      <p class="card-text"><%= Post.body %></p>
                      <p class="fw-lighter"><%= Post.catagory %></p>
                      <p class="fw-lighter"><%= Post.PostedOn %></p>
                      <a href="/delete/<%= Post.id %> " class="card-link link-danger">Delete</a>
                    </div>
                  </div>




                
                <% }) %> 

       
     </div>
</body>
</html>